{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.validate.enable": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "js/ts.implicitProjectConfig.checkJs": true,  // Enable type checking
  "cSpell.ignoreWords": [
    "doc",
    "jsdocs",
    "modules",
    "node",
    "resurse",
    "test"
  ]
}