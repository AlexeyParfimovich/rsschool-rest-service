{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/ | jsdocs |test | doc)"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "opts": {
    "resurse": true,
    "destination": "./jsdocs/"
  }
}